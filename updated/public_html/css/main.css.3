/* Global element styles - start */
html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, font, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td
{
    background: none repeat scroll 0 0 transparent;
    border: 0 none;
    font-size: 11px;
    margin: 0;
    outline: 0 none;
    padding: 0;
    vertical-align: baseline;
    z-index: 5;
}

html, body
{
    /*font-family: "Arial" , "Tahoma" , "Lucida Sans Unicode" , "Lucida Grande" , "Garuda" , "sans-serif";*/
    font-family: 'ProximaNovaLtRegular', Arial, Helvetica, sans-serif;
}
body
{
    line-height: 18px;
}
body{
    /*background-color: #000;*/
    background-image: url("../images/design/fon.png");
    background-repeat: repeat;
    /*margin:0;*/

}
body, html
{
    height: 100%;
}

img{
    border:0;
}

h3
{
    clear: both;
    color: #333333;
    font-size: 11px;
    font-weight: bold;
    margin: 0;
    font-size: 18px;
}

h3, h4, h5, h6 {
    line-height: 36px;
}
h1, h2, h3, h4, h5, h6 {
    color: #404040;
    font-weight: bold;
}

a {
    color: #0069D6;
    font-weight: inherit;
    line-height: inherit;
    text-decoration: none;
}
a:focus, a:hover
{
    color: #09f;
    text-decoration: none;
}

a:link, a:visited
{
    color: #EF0472;
}

a img:hover {
    /* opacity: 0.9;         */
}

dl, dt, dd, ul, form
{
    margin: 0;
    padding: 0;
}

ul, ol {
    margin: 0 0 18px 0px;
    list-style: none outside none;
}
li
{
    list-style: none;
}

iframe
{
    border: none;
    padding: 0;
}
/* Global element styles - end */


/* Generic classes - start*/
.center
{
    text-align: center;
}

.clear
{
    float: none;
    clear: both;
    height: 0px;
}

/* Generic classes - end*/


ul.profiles li {
    height: 200px;
    float: left;
    font-size: 12px;
    line-height: 1.3;
    margin-bottom: 10px;
    margin-right: 20px;
    position: relative;
    text-align: center;
    width: 220px;
}


/*ALL*/

.user_profile a {
    position: relative;
    display: inline-block;
}

.user_profile a img{
    height: 150px;
    width: 220px;
    border: 1px solid #ffffff;
    box-shadow: 0 0 2px #000000;
}


.user_profile .effect {
    position: absolute;
    display: none;
    color: white;
}

.user_profile .effect img {
    margin-top: 35%;
}

.user_profile:hover .effect {
    display: block;
    text-align: center;
    width: 100%;
    height: 100%;
    background-color: red;
    opacity: 0.4;

}

/* Header styles - start */
.topbar{
    width:100%;
    height:40px;
    border-top-style: solid;
    border-top-color: #f7305b;
    border-top-width: 5px;

}
.header{
    background-color: #181818;
    height:69px;
}

.users{
    width:740px;
}

.container:before, .container:after {
    content: "";
    display: table;
}
.container:after {
    clear: both;
}
.container:before, .container:after {
    content: "";
    display: table;
}
.container {
    margin-left: auto;
    margin-right: auto;
    width: 1200px;
    /*background-color: #181818; padding: 0 10px;*/
    vertical-align: middle;
}


.topbar h3 {
    position: relative;
}

.topbar h3 a {
    padding: 0;
    position: absolute;
    padding-top:15px;
}
.topbar h3 a {
    color: #FFFFFF;
    display: block;
    float: left;
    font-size: 20px;
    font-weight: 200;
    line-height: 1;
    margin-left: -20px;
    padding: 0px 20px 12px;
}
.topbar a {
    color: #BFBFBF;
    text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.25);
}
.topbar div > ul, .nav {
    display: block;
    float: right;
    left: 0;
    /*margin: 15px 10px 0 0;*/
    margin: 18px 0 0;
    position: relative;
}

.topbar .fill ul li {
    /*height: 40px;
    margin-left: 4px;
    padding-top: 8px;*/
}
.topbar div > ul > li, .nav > li {
    display: block;
    float: left;
}
.topbar div > ul > li, .nav > li {
    margin-right: 30px;
    display: block;
    float: left;
}
.topbar div > ul > li.last {
    margin-right: 0;
}

.topbar .fill ul li a {
    /*background-color: #222222;*/
    color: #FFFFFF;
    font-weight: bold;
    padding: 2px 10px;
    /*text-shadow: 2px 2px 2px #000000;*/
    font-size:15px;
    letter-spacing: 2px;
}
.topbar .fill ul li a:hover{
    background-color:#f6315c;
    /*text-shadow:1px 1px 1px #fff;*/
}

.topbar div > ul a, .nav a {
    display: block;
    float: none;
    line-height: 19px;
    padding: 10px 10px 11px;
    text-decoration: none;
}
.topbar div > ul a, .nav a {
    display: block;
    float: none;
    line-height: 19px;
    padding: 10px 10px 11px;
    text-decoration: none;
}
.topbar a {
    color: #BFBFBF;
    text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.25);
}

.effect img{
    width:24px;
    height:24px;
    position: relative;
}

.username{
    margin: 0px auto;
}
/* Header styles - end */